# Local Appium server
APPIUM_SERVER_URL=http://127.0.0.1:4723

# Default server environment: local_aos | local_ios | bs_aos | bs_ios
SERVER_ENV=local_aos

# App paths (optional, override capabilities)
ANDROID_APP=./apps/your-android-app.apk
IOS_APP=./apps/your-ios-app.app

# Device hints (optional)
UDID=
DEVICE_NAME=
PLATFORM_VERSION=

# BrowserStack credentials
BS_USERNAME=your_bstack_username
BS_ACCESS_KEY=your_bstack_access_key
BS_LOCAL=false
BS_PROJECT=Sample Project
BS_BUILD=Local Build
BS_SESSION_NAME=Smoke Test

# Parallelization
DEVICE_COUNT=1

# Extra capabilities as JSON string (e.g. '{"noReset": true}')
EXTRA_CAPS_JSON=

